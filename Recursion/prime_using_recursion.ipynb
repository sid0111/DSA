   "source": [
    "num = int(input('enter the number : '))\n",
    "\n",
    "def is_prime(num, i):\n",
    "    if num == i:\n",
    "        return True\n",
    "\n",
    "    if num % i == 0:\n",
    "        return False\n",
    "\n",
    "    return is_prime(num, i+1)\n",
    "\n",
    "if is_prime(num, 2):\n",
    "    print(f'{num} is prime')\n",
    "else:\n",
    "    print(f'{num} is not prime')"
   ]
