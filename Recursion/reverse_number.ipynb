    "#used array\n",
    "def rev_num(arr, l, n = 0):\n",
    "    if l == n:\n",
    "        return arr\n",
    "\n",
    "    arr[n], arr[-1 * (n+1)] = arr[-1 * (n+1)], arr[n]\n",
    "\n",
    "    return rev_num(arr, l, n+1)\n",
    "    \n",
    "n = input('enter number : ')\n",
    "arr = list(n)\n",
    "reverse = rev_num(arr, len(arr)//2)\n",
    "s = ''\n",
    "print(s.join(reverse))"
   ]
