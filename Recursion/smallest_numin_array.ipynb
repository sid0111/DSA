    "def findMinRec(A, n):\n",
    "    if n == 1:\n",
    "        return A[0]\n",
    "\n",
    "    return min(A[n-1], findMinRec(A, n-1))\n",
    "\n",
    "n = int(input('enter array size : '))\n",
    "list_num = []\n",
    "\n",
    "for i in range(n):\n",
    "    list_num.append(int(input(f'{i} : ')))\n",
    "\n",
    "min_ele = findMinRec(list_num, n)\n",
    "min_ele"
   ]
