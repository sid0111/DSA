    "num = int(input('number : '))\n",
    "copy = num\n",
    "sm = 0\n",
    "\n",
    "while copy>0:\n",
    "    rem = copy%10\n",
    "    sm+=rem\n",
    "    copy//=10\n",
    "    \n",
    "if(num%sm == 0):\n",
    "    print('harshad number')\n",
    "else:\n",
    "    print('not a harshad number')"
