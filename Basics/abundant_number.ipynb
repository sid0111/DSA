   "source": [
    "num = int(input('number : '))\n",
    "sm = 0\n",
    "\n",
    "for i in range(1, num//2+1):\n",
    "    if(num%i == 0):\n",
    "        sm+=i\n",
    "\n",
    "if(sm>num):\n",
    "    print('abundant number')\n",
    "else:\n",
    "    print('not a bundant number')"
   ]
