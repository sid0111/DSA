    "num = int(input('number : '))\n",
    "base = 1\n",
    "decimal = 0\n",
    "\n",
    "while num>0:\n",
    "    rem = num%10\n",
    "    decimal = decimal + rem * base\n",
    "    num//=10\n",
    "    base = base * 2\n",
    "\n",
    "print(decimal)"
   ]
