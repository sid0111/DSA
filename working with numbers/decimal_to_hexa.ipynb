   "source": [
    "num = int(input('number : '))\n",
    "hexa = []\n",
    "while num>0:\n",
    "    rem = num%16\n",
    "    if rem>=0 and rem<=10:\n",
    "        hexa.append(rem)\n",
    "    else:\n",
    "        n = chr(rem+55)\n",
    "        hexa.append(n)\n",
    "    num//=16\n",
    "\n",
    "for i in reversed(hexa):\n",
    "    print(i,end='')"
   ]
